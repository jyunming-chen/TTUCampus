{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": { "jest": true },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true
      }
    ],
    "import/no-extraneous-dependencies": "off"
  }
}
